<!--
	For rules causing false/broken MCB, see Model_View_Culture.com-falsemixed.xml.


	Mixed content:

		- css from $self *
		- css from fonts.googlapis.com *

		- Images from images *

	* Secured by us

-->
<ruleset name="Model View Culture.com (partial)" default_off="Needs ruleset tests">

	<target host="modelviewculture.com" />
	<target host="images.modelviewculture.com" />
	<target host="www.modelviewculture.com" />
		<!--
			Avoid broken MCB:
						-->
		<exclusion pattern="^http://(?:www\.)?modelviewculture.com/+(?!favicon\.ico|wp-content/|wp-includes/)" />

			<test url="http://modelviewculture.com/favicon.ico" />
			<test url="http://www.modelviewculture.com/favicon.ico" />


	<rule from="^http:"
		to="https:" />

</ruleset>
